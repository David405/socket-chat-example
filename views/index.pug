html 
  head
    title
      |socket.IO chat
      link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css')
      script.
          function staff() {
          var getUser = document.getElementById('users');

          fetch(`http://localhost:3000/user/all`)
            .then((resp) => resp.json())
            .then(function(data) {
              var users = data.message;
              return users.map(function(user) {
                  let item = document.createElement('li');
                  item.innerHTML = `${user.username}`;
                  getUser.appendChild(item);
                  console.log(item.innerHTML)
              })
            })
            .catch(function(error) {
              console.log(error);
            });
            }
  body(onload="staff();")
    h3.
      Users 
    ul(id="users")
      - var buttonClass=['btn', 'btn-lg', 'btn-primary', 'btn-block'];
          button(class=buttonClass type="submit") Sign in
  